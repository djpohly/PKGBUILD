-      fwrite ("\n *** ", 6, 1, ttyfo);
-      fputs (ALERT_START, ttyfo);
-      fwrite (text, (size_t) (eol - text), 1, ttyfo);
-      fputs (NORMAL_RESTORE, ttyfo);
